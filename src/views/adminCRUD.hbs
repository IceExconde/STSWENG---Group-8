<!-- CRUD Section -->
<section class="CRUD">
  <div class="crud-container">
    <div class="row">
      <!-- Picture -->
      <div class="col-md-6">
        <img src="path_to_your_image.jpg" class="img-fluid" alt="Property Image">
      </div>
      <!-- Form -->
      <div class="col-md-6">
        <form>
          <!-- Lot Size -->
          <div class="form-group">
            <label for="lotSize">Lot Size:</label>
            <input type="text" class="form-control" id="lotSize" name="lotSize">
          </div>
          <!-- Floor Size -->
          <div class="form-group">
            <label for="floorSize">Floor Size:</label>
            <input type="text" class="form-control" id="floorSize" name="floorSize">
          </div>
          <!-- Number of Floors -->
          <div class="form-group">
            <label for="numFloors">Number of Floors:</label>
            <input type="number" class="form-control" id="numFloors" name="numFloors">
          </div>
          <!-- Number of Rooms -->
          <div class="form-group">
            <label for="numRooms">Number of Rooms:</label>
            <input type="number" class="form-control" id="numRooms" name="numRooms">
          </div>
          <!-- Additional Features -->
          <div class="form-group">
            <label for="additionalFeatures">Additional Features:</label>
            <textarea class="form-control" id="additionalFeatures" name="additionalFeatures" rows="4"></textarea>
          </div>
          <!-- Submit Button -->
          <button type="submit" class="btn btn-primary">post</button>
        </form>
      </div>
    </div>
  </div>
</section>




